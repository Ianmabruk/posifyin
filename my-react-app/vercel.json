{
  "buildCommand": "cd my-react-app && npm run build",
  "outputDirectory": "my-react-app/dist",
  "installCommand": "cd my-react-app && npm install",
  "devCommand": "cd my-react-app && npm run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}